package SubsPackage;

{$R *.res}
{$R 'Subs2.dcr'}
{$R 'Subs.dcr'}
{$ALIGN 8}
{$ASSERTIONS ON}
{$BOOLEVAL OFF}
{$DEBUGINFO ON}
{$EXTENDEDSYNTAX ON}
{$IMPORTEDDATA ON}
{$IOCHECKS ON}
{$LOCALSYMBOLS ON}
{$LONGSTRINGS ON}
{$OPENSTRINGS ON}
{$OPTIMIZATION ON}
{$OVERFLOWCHECKS OFF}
{$RANGECHECKS ON}
{$REFERENCEINFO ON}
{$SAFEDIVIDE OFF}
{$STACKFRAMES OFF}
{$TYPEDADDRESS OFF}
{$VARSTRINGCHECKS ON}
{$WRITEABLECONST OFF}
{$MINENUMSIZE 1}
{$IMAGEBASE $4000000}
{$DESCRIPTION 'Borland User Components'}
{$IMPLICITBUILD OFF}

requires
  rtl,
  vcl,
  dbrtl,
  visualclx,
  bdertl,
  ibxpress,
  dsnap,
  vcldb,
  indy;

contains
  Subs in 'Subs.pas',
  WinSock2 in 'winsock2.pas',
  Subs3 in 'Subs3.pas',
  Subs2 in 'Subs2.pas',
  Subs4 in 'Subs4.pas';

end.
